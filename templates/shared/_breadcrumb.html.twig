<ol class="breadcrumb bg-light py-3 px-2">
    {% for link in links %}
        {% if loop.last %}
            <li class="breadcrumb-item active text-muted">{{ link.name }}</li>
        {% else %}
            <li class="breadcrumb-item">
                <a href="{{ link.href }}">{{ link.name }}</a>
            </li>
        {% endif %}
    {% endfor %}
</ol>
